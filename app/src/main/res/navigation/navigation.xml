<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/navigation"
            app:startDestination="@id/authFragment">

    <fragment android:id="@+id/authFragment" android:name="ru.belenkov.headsandhandstest.authorization.AuthFragment"
              android:label="fragment_auth" tools:layout="@layout/fragment_auth">
        <action android:id="@+id/action_authFragment_to_registrationFragment"
                app:destination="@id/registrationFragment"/>
        <action android:id="@+id/action_authFragment_to_landingFragment" app:destination="@id/landingFragment"/>
    </fragment>
    <fragment android:id="@+id/landingFragment" android:name="ru.belenkov.headsandhandstest.landing.LandingFragment"
              android:label="fragment_landing" tools:layout="@layout/fragment_landing"/>
    <fragment android:id="@+id/registrationFragment"
              android:name="ru.belenkov.headsandhandstest.registration.RegistrationFragment"
              android:label="fragment_registration" tools:layout="@layout/fragment_registration"/>
</navigation>